<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pack.mappers.UserMapper">
    <select id="selectObject" parameterType="java.lang.Integer" resultType="com.pack.entity.User">
        SELECT * FROM user u WHERE u.id = #{VALUE };
    </select>
</mapper>